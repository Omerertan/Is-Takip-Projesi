@model AciliyetAddDto
@{
    ViewData["Title"] = "EkleAciliyet";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<form method="post" class="w-75 mx-auto p-3">
    <div class="form-group">
        <label asp-for="@Model.Tanim">Tanım : </label>
        <span asp-validation-for="@Model.Tanim" class="text-danger"></span>
        <input asp-for="Tanim" type="text" class="form-control" />
    </div>
    <button type="submit" class="btn btn-sm btn-dark mt-2"> <i class="fas fa-check mr-1"></i> Kaydet </button>
</form>

@section Javascript
 {
    <script src="~/lib/jqueryValidate/jquery.validate.min.js"></script>
    <script src="~/lib/jqueryUnob/jquery.validate.unobtrusive.min.js"></script>
}

